#include<stdio.h>
int main()
{
    int a,b,n,i,c;
    printf("enter starting two numbers \n");
    scanf("%d %d",&a,&b);
    printf("enter how much elements in series you want to print");
    scanf("%d",&n);
    printf("Fibonacci series \n%d  %d  ",a,b);
    for(i=0;i<n;i++)
    {
        c=a+b;
        a=b;
        b=c;
        printf("%d  ",c);
    }
    return 0;
} 
